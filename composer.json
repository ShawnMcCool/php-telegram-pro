{
  "name": "php-telegram-pro/php-telegram-pro",
  "description": "The professional PHP Telegram SDK for interacting with the Telegram API",
  "authors": [
    {
      "name": "Shawn McCool",
      "email": "shawnmc@gmail.com",
      "homepage": "https://shawnmc.cool/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3@dev",
    "shawnmccool/phpunit-result-printer": "^1.1"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "TelegramPro\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "tests/functions.php"
    ]
  },
  "license": "none"
}
